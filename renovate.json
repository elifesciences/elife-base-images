{
  "extends": [
    "config:base"
  ],
  "assignees": [
      "scottaubrey"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^php_image_digests.txt"],
      "matchStrings": ["PHP_IMAGE_.+=(?<depName>.*?):(?<currentValue>.*?)@(?<currentDigest>sha256:[a-f0-9]+)"],
      "datasourceTemplate": "docker"
    }
  ],
  "includePaths": [
    "php_image_digests.txt",
    ".github/**"
  ]
}
